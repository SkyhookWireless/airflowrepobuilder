[airflowrepo]
name=Airflow $releasever - $basearch
baseurl=file://@@@REPOBASEDIR@@@/airflowrepo/$releasever/$basearch
enabled=0
gpgcheck=0
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-schol
# Set very, very short for RPM building
metadata_expire=1

[airflowrepo-source]
name=Airflow $releasever - Source
baseurl=file://@@@REPOBASEDIR@@@/airflowrepo/$releasever/SRPMS
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-schol
# Set very, very short for RPM building
metadata_expire=1
