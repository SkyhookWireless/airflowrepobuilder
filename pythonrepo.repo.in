[pythonrepo]
name=RT4 $releasever - $basearch
baseurl=file://@@@REPOBASEDIR@@@/pythonrepo/$releasever/$basearch
enabled=0
gpgcheck=0
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-schol
# Set very, very short for RPM building
metadata_expire=1

[pythonrepo-source]
name=RT4 $releasever - Source
baseurl=file://@@@REPOBASEDIR@@@/pythonrepo/$releasever/SRPMS
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-schol
# Set very, very short for RPM building
metadata_expire=1
